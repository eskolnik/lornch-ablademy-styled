<div class="row">
  <div class="small-12 large-6 large-offset-3 columns">
    <h1 class="clinic-header">List of Clinics!</h1>

    <ul>
      <% @clinics.each do |clinic| %>
        <li class="clinic-link">
          <strong><%= link_to clinic.name, clinic_path(clinic) %></strong>: <%= clinic.speaker %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
